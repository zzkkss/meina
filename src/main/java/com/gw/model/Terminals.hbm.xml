<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gw.model">
	<class
		name="Terminals"
		table="terminals"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="Id"
		>
			<generator class="native"/>
		</id>

		<property
			name="TId"
			column="tId"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="Tname"
			column="tname"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="DeviceType"
			column="deviceType"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="DeviceState"
			column="deviceState"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DifKey"
			column="difKey"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="DeviceSoftVersion"
			column="deviceSoftVersion"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="GpsCardNum"
			column="gpsCardNum"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="Diftype"
			column="diftype"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Cardgpstype"
			column="cardgpstype"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Lat"
			column="lat"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="Utc"
			column="utc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Lng"
			column="lng"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="Hgt"
			column="hgt"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="PositionState"
			column="positionState"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="RawData"
			column="rawData"
			type="string"
			not-null="true"
			length="1024"
		/>
		<property
			name="DataLength"
			column="dataLength"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DataType"
			column="dataType"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Introductions"
			column="Introductions"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="OnlineState"
			column="onlineState"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="TcpSession"
			column="TcpSession"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="UdpSession"
			column="UdpSession"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>


	</class>	
</hibernate-mapping>