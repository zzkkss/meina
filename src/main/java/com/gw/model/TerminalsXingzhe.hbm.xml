<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gw.model">
	<class
		name="TerminalsXingzhe"
		table="terminals_xingzhe"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="Id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Tname"
			column="tname"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="DeviceType"
			column="deviceType"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="Parameter"
			column="parameter"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="RawData"
			column="rawData"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="DataLength"
			column="dataLength"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DataType"
			column="dataType"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="TcpSession"
			column="tcpSession"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="UdpSession"
			column="udpSession"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="Lat"
			column="lat"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="Lng"
			column="lng"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="Hgt"
			column="hgt"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="Speed"
			column="speed"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="Distance"
			column="distance"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>
		<property
			name="StepNumber"
			column="stepNumber"
			type="java.lang.Double"
			not-null="true"
			length="22"
		/>


	</class>	
</hibernate-mapping>